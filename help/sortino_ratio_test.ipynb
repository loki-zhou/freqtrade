    "from empyrical import sortino_ratio\n",
    "import numpy as np\n",
    "a = [1.0, 1.01, 0.95, 0.96, 0.98, 0.99, 1.1, 1.2, 1.3]\n",
    "returns = np.diff(a)\n",
    "reward = sortino_ratio(returns, annualization=365*24)\n",
    "print(reward)\n",
    "print(reward if np.isfinite(reward) else 0)"
   ]
  }
